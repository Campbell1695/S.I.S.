<%= @admin.name %>, <%= @admin.email %>

<%= link_to "Settings", edit_admin_path(current_admin) %>
<%= link_to "All events", events_path, class: "btn btn-lg btn-primary" %>
<h1 id=""><%= link_to 'log out', logout_path(session), method: :delete, data: { confirm: 'Are you sure?' } %></h1>

<button id="" onclick="e.network()" class="button">Network</button>
<button id="" onclick="e.workshop()" class="button">Workshops</button>
<button id="" onclick="e.motivation()" class="button">Motivation</button>
<button id="" onclick="e.resource()" class="button">Resources</button>
<button id="" onclick="e.howTo()" class="button">How To's</button>

<div id="networkModal" class="modal">
  <h1>Network</h1>
  <form action="/events" enctype="multipart/form-data" method="post">
    <input type="hidden" name="[create]content" value="Network">
    <input type="text" name="[create]title" placeholder="Title">
    <br>
    <p>Description:</p>
    <input type="text" name="[create]description">

    <p>Host:</p>
    <input type="text" name="[create]host">

    <p>Misc:</p>
    <input type="text" name="[create]misc">

    <p>Setting:</p>
    <input type="text" name="[create]setting">

    <p>Cost:</p>
    <input type="text" name="[create]cost">

    <p>picture:</p>
    <input type="file" name="[create]image">

    <input type="submit" name="" value="submit">
  </form>
</div>

